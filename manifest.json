{
  "name": "LeakFinder",
  "description": "by s7ck Team",
  "version": "0.0.3", 
  
  "background": {
      "scripts": [
          "js/jquery-3.4.1.min.js",
          "js/bootstrap-3.3.7.min.js",
          "js/background.js"
      ]
  },

//  "default_popup": "popup.html",

  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications",
    "contextMenus"
  ],

  "icons": 
  { 
    "16": "image/icon16.png",             
    "48": "image/icon48.png",            
    "128": "image/icon128.png" 
  },  

  "browser_action": {
	   "default_icon": "image/icon16.png",
     "default_popup": "popup.html"
	},

  "manifest_version":2
}

